      "from math import factorial \n",
      "from pprint import pprint\n",
      "\n",
      "def gamma_factorial(t):\n",
      "    if t >= 1:\n",
      "        fact = factorial(t - 1)\n",
      "    return fact\n",
      "\n",
      "pprint(gamma_factorial)\n",
      "        "
